<!DOCTYPE html>
<html>
<head>
  <title>College Dashboard</title>
</head>
<body>

<h2>College Dashboard</h2>

<ul>
  <li><a href="/college/profile">View Profile</a></li>
  <li><a href="/college/students">View Students</a></li>

</ul>

<button onclick="logout()">Logout</button>

<script>
  async function logout() {
    await fetch("/api/auth/logout", {
      method: "POST",
      credentials: "include"
    });

    window.location.href = "/login";
  }
</script>


</body>
</html>
